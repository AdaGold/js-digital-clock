{
  "parser": "babel-eslint",
  "plugins": ["jest", "react"],
  "rules": {
    "max-len": [1, 120, 2, {ignoreComments: true}],
    "no-console": 0,
    "no-var": 1,
    "jsx-a11y/href-no-hash": [0]
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true,
    "jest/globals": true
  }
}
